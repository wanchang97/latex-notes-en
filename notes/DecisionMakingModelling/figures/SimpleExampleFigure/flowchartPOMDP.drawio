        <mxCell id="tmotjHpgioBApPOrCeOB-59" value="" style="rounded=0;whiteSpace=wrap;html=1;strokeColor=#97D077;strokeWidth=4;align=left;" parent="tmotjHpgioBApPOrCeOB-70" vertex="1">
          <mxGeometry x="30" width="630" height="325" as="geometry" />
